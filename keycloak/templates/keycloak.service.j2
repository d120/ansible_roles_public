# Managed by Ansible. DO NOT CHANGE!

[Unit]
Description=Keycloak
Wants=network.target
After=network.target

[Service]
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=keycloak
User=keycloak
Group=keycloak
ExecStart={{ keycloak.settings.dest }}/bin/standalone.sh
WorkingDirectory={{ keycloak.settings.dest }}
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
